{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@screens/*": ["src/screens/*"],
      "@utils/*": ["src/utils/*"],
      "@assets/*": ["src/assets/*"],
      "@navigations/*": ["src/navigations/*"],
      "@hooks/*": ["src/hooks/*"],
      "@configs/*": ["src/configs/*"],
      "@type/*": ["src/types/*"]
    }
  }
}
